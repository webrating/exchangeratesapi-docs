{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "willow",
  "name": "Exchange Rates API",
  "colors": {
    "primary": "#4742A9",
    "light": "#5B56BD",
    "dark": "#252258"
  },
  "favicon": "/logo.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Getting Started",
        "groups": [
          {
            "group": "Introduction",
            "pages": [
              "introduction",
              "quickstart",
              "authentication",
              "rate-limits"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "openapi": "/openapi.json",
        "groups": [
          {
            "group": "Public Endpoints",
            "pages": [
              "GET /",
              "GET /status", 
              "GET /symbols",
              "GET /convert"
            ]
          },
          {
            "group": "Authenticated Endpoints",
            "pages": [
              "GET /latest",
              "GET /latest/{currency}",
              "GET /timeseries",
              "GET /{date}",
              "GET /{date}/{currency}"
            ]
          }
        ]
      },
      {
        "tab": "SDKs",
        "groups": [
          {
            "group": "Client Libraries",
            "pages": [
              "sdks/javascript"
            ]
          }
        ]
      },
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Best Practices",
            "pages": [
              "guides/best-practices"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Dashboard",
          "href": "https://app.exchangeratesapi.com.au/dashboard",
          "icon": "chart-line"
        },
        {
          "anchor": "Status",
          "href": "https://status.exchangeratesapi.com.au",
          "icon": "activity"
        },
        {
          "anchor": "Support",
          "href": "mailto:support@exchangeratesapi.com.au",
          "icon": "envelope"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo.svg",
    "dark": "/logo-dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Website",
        "href": "https://exchangeratesapi.com.au"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "https://app.exchangeratesapi.com.au/login"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/webrating/exchangeratesapi-docs",
      "linkedin": "https://linkedin.com/company/exchangeratesapi"
    }
  }
