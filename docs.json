{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "willow",
  "name": "Exchange Rates API",
  "colors": {
    "primary": "#4742A9",
    "light": "#5B56BD",
    "dark": "#252258"
  },
  "logo": {
    "light": "/logo.png",
    "dark": "/logo-dark.png"
  },
  "favicon": "/favicon.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Getting Started",
        "groups": [
          {
            "group": "Introduction",
            "pages": [
              "introduction",
              "quickstart",
              "authentication",
              "support"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "openapi": "/openapi.json",
        "groups": [
          {
            "group": "Public Endpoints",
            "pages": [
              "api-reference/public/status",
              "api-reference/public/symbols",
              "api-reference/public/convert-free"
            ]
          },
          {
            "group": "Authenticated Endpoints",
            "pages": [
              "api-reference/authenticated/latest",
              "api-reference/authenticated/latest-currency",
              "api-reference/authenticated/timeseries",
              "api-reference/authenticated/historical-date",
              "api-reference/authenticated/historical-date-currency",
              "api-reference/authenticated/convert-authenticated"
            ]
          }
        ]
      },
      {
        "tab": "SDKs",
        "groups": [
          {
            "group": "Client Libraries",
            "pages": [
              "sdks/javascript",
              "sdks/python",
              "sdks/php",
              "sdks/curl"
            ]
          }
        ]
      },
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Best Practices",
            "pages": [
              "guides/best-practices",
              "guides/troubleshooting",
              "guides/rate-limits"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Home",
          "href": "https://www.exchangeratesapi.com.au",
          "icon": "house"
        },
        {
          "anchor": "Dashboard",
          "href": "https://app.exchangeratesapi.com.au/dashboard",
          "icon": "chart-line"
        },
        {
          "anchor": "Status",
          "href": "https://status.exchangeratesapi.com.au",
          "icon": "wifi"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "www.exchangeratesapi.com.au",
        "href": "https://www.exchangeratesapi.com.au"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "https://app.exchangeratesapi.com.au/login"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "links": [
      {
        "header": "Legal",
        "items": [
          {
            "label": "Legal",
            "href": "https://docs.exchangeratesapi.com.au/legal/legal"
          },
          {
            "label": "Compliance",
            "href": "https://docs.exchangeratesapi.com.au/legal/compliance"
          }
        ]
      }
    ]
  }
}