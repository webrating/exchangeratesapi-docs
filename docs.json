{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "willow",
  "name": "Exchange Rates API",
  "colors": {
    "primary": "#4742A9",
    "light": "#5B56BD",
    "dark": "#252258"
  },
  "logo": {
    "light": "/logo.png",
    "dark": "/logo-dark.png"
  },
  "favicon": "/favicon.png",
  "navigation": {
    "tabs": [
      {
        "tab": "Getting Started",
        "groups": [
          {
            "group": "Introduction",
            "pages": [
              "introduction",
              "quickstart",
              "authentication",
              "rate-limits"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "openapi": "/openapi.json",
        "groups": [
          {
            "group": "Public Endpoints",
            "pages": [
              "api-reference/public/root",
              "api-reference/public/status",
              "api-reference/public/symbols",
              "api-reference/public/convert-free"
            ]
          },
          {
            "group": "Authenticated Endpoints",
            "pages": [
              "api-reference/authenticated/latest",
              "api-reference/authenticated/latest-currency",
              "api-reference/authenticated/timeseries",
              "api-reference/authenticated/historical-date",
              "api-reference/authenticated/historical-date-currency",
              "api-reference/authenticated/convert-authenticated"
            ]
          }
        ]
      },
      {
        "tab": "SDKs",
        "groups": [
          {
            "group": "Client Libraries",
            "pages": [
              "sdks/javascript"
            ]
          }
        ]
      },
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Best Practices",
            "pages": [
              "guides/best-practices"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Home",
          "href": "https://exchangeratesapi.com.au",
          "icon": "house"
        },
        {
          "anchor": "Dashboard",
          "href": "https://app.exchangeratesapi.com.au/dashboard",
          "icon": "chart-line"
        },
        {
          "anchor": "Status",
          "href": "https://status.exchangeratesapi.com.au",
          "icon": "wifi"
        },
        {
          "anchor": "Support",
          "href": "mailto:support@exchangeratesapi.com.au",
          "icon": "envelope"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Website",
        "href": "https://exchangeratesapi.com.au"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get API Key",
      "href": "https://app.exchangeratesapi.com.au/login"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "github": "https://github.com/webrating/exchangeratesapi-docs",
      "linkedin": "https://linkedin.com/company/exchangeratesapi"
    }
  }
}